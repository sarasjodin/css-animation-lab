{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.scss": [
      "@parcel/transformer-sass"
    ],
    "*.js": [
      "@parcel/transformer-js"
    ],
    "*.html": [
      "@parcel/transformer-html"
    ],
    "*.css": [
      "@parcel/transformer-css"
    ]
  },
  "optimizers": {
    "*.html": [
      "@parcel/optimizer-htmlnano"
    ],
    "*.css": [
      "@parcel/optimizer-css"
    ]
  }
}
